<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Proactive" parent="android:Theme.Material.Light.NoActionBar" />

    <style name="AppTheme.NavigationActivity" parent="Theme.AppCompat.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:actionBarStyle">@style/CustomActionBar</item>
    </style>

    <style name="CustomActionBar" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/teal_700</item>
    </style>

    <style name="CustomDatePicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">#053D57</item>
    </style>

    <style name="CustomTimePicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">#053D57</item>
    </style>
  <!-- Base application theme. -->
    <style name="Base.Theme.Proactive" parent="Theme.Material3.DayNight.NoActionBar">
    </style>

    <style name="ProgressDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


</resources>